include "../std/std.t++"

# fibonacci #
def i32 fib(i32 n) = (
    def i32 a1 = 1
    def i32 a2 = 0
    for [0, n - 1] (
        def b = a1 + a2
        a2 = a1
        a1 = b
    )
)

def i32 N = 10

# entry point #
def i32 main([[i8]] args) = (
    for [0, args.size] -> i
        printf("%5d | %s\n", i, args[i])

    def fibN = fib(N)
    printf("fib(%d) = %d\n", N, fibN)
    0
)
